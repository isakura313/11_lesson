jQuery(document).ready(function() {
	var cache = {},
	lastXhr;
	$("#searchinput" ).autocomplete({
		minLength: 2,
		source: function( request, response ) {
			var term = request.term;
			if ( term in cache ) {
				response( cache[ term ] );
				return;
			}
			lastXhr = $.getJSON( "/fastsearch.php", request, function( data, status, xhr ) {
				cache[ term ] = data;
				if ( xhr === lastXhr ) {
					response( data );
				}
				$(".ui-autocomplete").children(".ui-menu-item").click(function() {
					//	alert("12345");
					var text = $(this).children('a').text();
					$('#searchinput').val(text);
					document.getElementById('search').submit();
				});
			});
		}
	});



});





