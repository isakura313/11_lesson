jQuery(document).ready(function() {




	var add_idle = true;
	jQuery('.bask').click(function() {
		if (! add_idle) return false;
		var cbtrig = jQuery(this);
		
		add_idle = false;
		if (jQuery(this).attr('href') == cat_link_const) {
			yaCounter19723555.reachGoal('in_cart_buy');
			add_idle = true;
		} else {
			_pid = jQuery(this).attr('rel');
			jQuery.post('/ajax_port.php', {action: 'is_group_product', pid: _pid}, function(resp) {
				resp = eval('('+resp+')');
				if (resp.products.length && ! $('.product_cart').length) {
					jQuery('body').append(resp.window);
					reInitGroupBuy(function() {
						for(var i=0; i<resp.products.length; i++) {
