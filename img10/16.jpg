			jQuery('#shopping_cart .cart_items').html(_text);
			_click.parent().parent().parent().hide('fast');
		});
	});
});



function addinbasket(pid, sale) {
	
	if (sale == undefined) {
		var sale = false;
	}
	
	var cookie_date = new Date();
	cookie_date.setTime(cookie_date.getTime() + 2592000000);
	document.cookie = 'startsess=1; expires=' + cookie_date.toGMTString() + '; path=/;';
	_cou = $('#qty').val();
	if (isNaN(_cou)) _cou=1;
	document.getElementById('basket').p_id.value = pid;
	document.getElementById('basket').quantity.value = _cou;
	//if (sale) alert(jQuery('#basket').serialize());
	$.ajax({
		type : 'POST',
		url  : '/ajax/?type=addbasket',
		data : jQuery('#basket').serialize()+(sale ? '&sale=true' : '')
	}).done(function(msg) {
		_text = (msg>1)?(msg>4)?'ов':'а':'';
		_text = (msg==0)?'Пусто': msg+' товар'+_text;
		jQuery('#shopping_cart .cart_items').html(_text);
		//alert( "Data Saved: " + msg +_text);
		
		var ppm = $('#ppm'+pid);
		if (ppm.length) {
			ecomOnAddEvent(pid, ppm.attr('pname'), ppm.attr('pprice'));
		} else {
			ppm = $('#p'+pid);
			ecomOnAddEvent(pid, ppm.find('.name').text(), ppm.prev().find('.price').attr('product_price'));
		}
		
	});
	console.log("vt pid="+pid);
	if ($("a[rel="+pid+"]").parent(".product_cart").length==0)
		$("a[rel="+pid+"]").replaceWith('<div class="but_cart"><span class="basket_minus" data-id="'+pid+'" data-kol="1">-</span><span class="kol_in_cart">1 шт.</span><span class="basket_plus" data-id="'+pid+'">+</span></div>');
	else
		$("a[rel="+pid+"]").replaceWith('<div class="prod_but_cart"><span class="kol_in_cart" onclick="location.assign(\'/checkout/\');"><b>В корзине 1 шт.</b>Перейти в корзину</span><span class="basket_plus" data-id="'+pid+'">+1 шт.</span></div>');
	return false;
};


