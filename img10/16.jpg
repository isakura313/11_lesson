					console.log("FOOT SINGLE pid="+_pid);
					console.log("pid="+_pid);
					yaCounter19723555.reachGoal('add_to_cart');
					jQuery('.bask[rel="'+_pid+'"]').removeClass('green_btn').addClass('btn13').html('В КОРЗИНЕ').attr('href',cat_link_const);
